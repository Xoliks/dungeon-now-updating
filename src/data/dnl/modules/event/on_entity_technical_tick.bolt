from reapermc:event_handler import Event, listener
from reapermc:flag_handler import Flag

on_entity_technical_tick = Event('on_entity_technical_tick')
on_entity_technical_tick.set_handler_path(f"reaper_framework:__internal__/event_handler/on_entity_technical_tick")

@on_entity_technical_tick.handler
def handler():
    execute function on_entity_technical_tick.generate_path('entity_living_tick'):
        function ../tick/technical:
            on_entity_technical_tick.trigger()