infinite_invisibility = {
    Id: 14,
    Duration: 999999,
    Amplifier: 1,
    ShowParticles: false,
}

def summon_chicken_army(n):
    for i in range(n):
        summon pig ~i ~ ~ {
            Tags: [f"quack{i}"],
            Passengers: [{
                id: zombie,
                IsBaby: true,
                ActiveEffects: [infinite_invisibility]
            }]
        }

function ./hey:
    say Go forth, my minions!
    summon_chicken_army(16)


